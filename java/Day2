import java.util.*;
class day2
{

  public static void display1()     // can be call directly 
	{
		System.out.println("in am in static methord");
	} 


	public void display()        //can be called with object 
	{ 
		System.out.println("i am in non static methord");
	} 


	public static void main(String args[])
	{
		day2 f = new day2();    //stored in heep memory 
		 // creating object 
		
		int x = 10;     // stack memory 
		f.display();
		display1();
	}
}

		
	
